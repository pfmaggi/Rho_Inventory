<div class="container">
  <h2>Inventories</h2>
  <a href="<%= Rho::Application.startURI %>" class="btn btn-primary"><i class="glyphicon glyphicon-home" ></i>&nbsp Home</a>
  <a href="<%= url_for :action => :new %>" class="pull-right btn btn-primary" ><i class="glyphicon glyphicon-plus-sign" ></i>&nbsp New</a>

  <ul class="list-group">
    <% @inventories.each do |inventory| %>
      <li class="list-group-item">
        <a href="<%= url_for :action => :show, :id => inventory.object %>">
          <%= inventory.SKU %>
        </a>
      </li>
    <% end %>
  </ul>
</div>
