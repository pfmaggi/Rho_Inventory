<div class="container">
  <h2>Products</h2>
  <a href="<%= Rho::Application.startURI %>" class="btn btn-primary"><i class="glyphicon glyphicon-home" ></i>&nbsp Home</a>
  <a href="<%= url_for :action => :new %>" class="btn btn-primary" ><i class="glyphicon glyphicon-plus-sign" ></i>&nbsp New</a>
  <ul class="list-group">
    <% @productcatalogs.each do |productcatalog| %>
    
        <li class="list-group-item">
          <a href="<%= url_for :action => :show, :id => productcatalog.object %>">
            <%= productcatalog.Name %>
          </a>        
        </li>
    
    <% end %>
  </ul>

</div>
