<div class="container">
  <h2><%= @inventory.SKU %></h2>
  <a href="<%= url_for :action => :index%>" class="btn btn-primary"><i class="glyphicon glyphicon-arrow-left" ></i>&nbsp Back</a>
  <a href="<%= url_for :action => :edit, :id => @inventory.object %>" class="btn btn-primary"><i class="glyphicon glyphicon-star" ></i>&nbsp Edit</a>

  <div class="form-group">
    <label class="control-label" for="inventory[SKU]">SKU</label>
    <div class="controls">
      <p><%= @inventory.SKU %></p>
    </div>
  </div>
  <div class="form-group">
    <label class="control-label" for="inventory[Quantity]">Quantity</label>
    <div class="controls">
      <p><%= @inventory.Quantity %></p>
    </div>
  </div>
</div>
