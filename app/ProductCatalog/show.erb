<div class="container">
  <h2><%= @productcatalog.Name %></h2>
  <a href="<%= url_for :action => :index%>" class="btn btn-primary"><i class="glyphicon glyphicon-arrow-left" ></i>&nbsp Back</a>
  <a href="<%= url_for :action => :edit, :id => @productcatalog.object %>" class="btn btn-primary"><i class="glyphicon glyphicon-star" ></i>&nbsp Edit</a>

  <div class="form-group">
    <label class="control-label" for="productcatalog[Name]">Name</label>
    <div class="controls">
      <p><%= @productcatalog.Name %></p>
    </div>
  </div>
  <div class="form-group">
    <label class="control-label" for="productcatalog[SKU]">SKU</label>
    <div class="controls">
      <p><%= @productcatalog.SKU %></p>
    </div>
  </div>
  <div class="form-group">
    <label class="control-label" for="productcatalog[Image]">Image</label>
    <div class="controls">
      <img src="<%= @productcatalog.Image %>"/>
    </div>
  </div>
</div>
